[run]
source = backend
omit =
    */tests/*
    */test_*.py
    *_test.py
    */site-packages/*
    */dist-packages/*
    */.venv/*
    */venv/*
    */__pycache__/*
    */migrations/*
    backend/__init__.py
    backend/*/__init__.py
    backend/utils/notif_util/*
    backend/routers/notification.py
    backend/wrappers/storage.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov